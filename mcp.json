{
  "mcpServers": {
    "fetch": {
      "command": "uvx",
      "args": ["mcp-server-fetch"],
      "env": {}
    },
    "mcp-youtube": {
      "command": "npx",
      "args": ["@anaisbetts/mcp-youtube"]
    },
    "time": {
      "command": "uvx",
      "args": [
        "mcp-server-time",
        "--local-timezone=Asia/Seoul"
      ]
    }
  }
}